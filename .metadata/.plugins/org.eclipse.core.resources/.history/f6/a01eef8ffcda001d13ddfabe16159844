package gfg;

import java.util.Arrays;

public class MoveZerosToEnd {

	public static void main(String[] args) {
		int arr[]= {1,0,2,3,2,0,0,4,5,1};
		int n=arr.length;
		int count =0;
		int brr[]=new int[n];
		for(int i=0;i<n;i++)
		{
			if(arr[i]!=0)
			{
				brr[i]=arr[i];
			}
		}
		System.out.println(Arrays.toString(brr));
	}

}
